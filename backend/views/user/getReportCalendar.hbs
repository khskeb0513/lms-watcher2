<!doctype html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
</head>
<body>
<h1>
  /user/getReportcalendar
</h1>
<p>
  ========================================
  <br>
  course title
  <br>
  report title
  <br>
  ====================
  <br>
  submit status
  <br>
  endDate || lateEndDate
  <br>
  ========================================
</p>
{{#each calendars}}
  <p>
    ========================================
    <br>
    {{courseTitle}}
    <br>
    {{reportTitle}}
    <br>
    ====================
    <br>
    {{#if submitStatus}}
      finished
    {{/if}}
    {{^if submitStatus}}
      <b>not finished</b>
    {{/if}}
    <br>
    {{endDate}} || {{lateEndDate}}
    <br>
    ========================================
    <br>
  </p>
{{/each}}
</body>
</html>
